<Page x:Class="WPF_POS.Pages.InventoryReport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_POS.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="InventoryReport">

    <Grid>
        <DataGrid x:Name="datagrid" HorizontalAlignment="Center" Height="274" Margin="0,139,0,0" VerticalAlignment="Top" Width="616" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Label Content="INVENTORY REPORT" HorizontalAlignment="Center" Height="71" Margin="0,24,0,0" VerticalAlignment="Top" Width="454"/>

    </Grid>
</Page>
